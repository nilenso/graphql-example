{
  "name": "backend",
  "version": "1.0.0",
  "description": "An example graphql server meant to deployed on serverless infrastructure",
  "main": "./src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "node dotenv/config dist/build.js",
    "prestart": "node esbuild.config.js",
    "dev": "nodemon -r dotenv/config src/index.ts",
    "createTables": "ts-node -r dotenv/config src/createTables.ts",
    "test": "DOTENV_CONFIG_PATH=.env.test jest --setupFiles dotenv/config ./specs/setup.ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.39.0",
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "apollo-server": "^3.5.0",
    "class-validator": "^0.13.1",
    "esbuild": "^0.13.9",
    "graphql": "^15.3.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-tools/utils": "^8.5.3",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.6",
    "@types/uuid": "^8.3.1",
    "esbuild-node-externals": "^1.3.0",
    "eslint": "^8.1.0",
    "faker": "^5.5.3",
    "fast-check": "^2.19.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4",
    "watchman": "^1.0.0"
  }
}
