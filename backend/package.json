{
  "name": "serverless-graphql-example",
  "version": "1.0.0",
  "description": "An example graphql server meant to deployed on serverless infrastructure",
  "main": "./src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "node dotenv/config dist/build.js",
    "prestart": "node esbuild.config.js",
    "dev": "nodemon -r dotenv/config src/index.ts",
    "createTables": "ts-node -r dotenv/config src/createTables.ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.39.0",
    "ajv": "^8.6.3",
    "esbuild": "^0.13.9",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^16.0.0",
    "morgan": "^1.10.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.6",
    "@types/uuid": "^8.3.1",
    "esbuild-node-externals": "^1.3.0",
    "eslint": "^8.1.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4",
    "watchman": "^1.0.0"
  }
}
